{
  "name": "@mongodb-js/react-mapbox-gl-leaflet",
  "version": "1.0.0",
  "description": "React wrapper of mapbox-gl-leaflet for react-leaflet.",
  "author": "MongoDB",
  "license": "MIT",
  "main": "lib/react-mapbox-gl-leaflet.js",
  "module": "src/react-mapbox-gl-leaflet/index.js",
  "scripts": {
    "build:demo": "cross-env webpack --config ./config/webpack.prod.config.js",
    "build:dll": "cross-env webpack --config ./config/webpack.dll.config.js",
    "build:lib": "cross-env webpack --config ./config/webpack.lib.config.js",
    "check": "mongodb-js-precommit \"./src/**/*{.js,.jsx}\"",
    "check:staged": "lint-staged",
    "clean": "rimraf build && rimraf lib && rimraf demo",
    "cover": "cross-env nyc npm run test && nyc report --reporter=lcov",
    "predist": "cross-env npm run clean && npm run build:dll",
    "dist": "cross-env NODE_ENV=production npm run build:lib",
    "postdist": "cross-env NODE_ENV=production npm run build:demo",
    "serve": "cross-env NODE_ENV=development node dev-server.js",
    "start": "cross-env NODE_ENV=development npm run serve",
    "test": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\" --interactive false",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\" --watch",
    "test:karma": "cross-env NODE_ENV=test karma start"
  },
  "precommit": [
    "check:staged"
  ],
  "lint-staged": {
    "src/**/*{.js,.jsx}": "mongodb-js-precommit"
  },
  "dependency-check": {
    "ignore": [
      "react",
      "react-dom",
      "containers",
      "lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mongodb-js/react-mapbox-gl-leaflet.git"
  },
  "bugs": {
    "url": "https://github.com/mongodb-js/react-mapbox-gl-leaflet/issues"
  },
  "homepage": "https://github.com/mongodb-js/react-mapbox-gl-leaflet#readme",
  "keywords": [
    "react",
    "react-leaflet",
    "mapbox",
    "mapbox-gl",
    "leaflet"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
