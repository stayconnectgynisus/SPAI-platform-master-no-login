    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.0",
    "eslint-config-mongodb-js": "^3.0.1",
    "eslint-plugin-mocha": "^5.3.0",
    "express": "^4.16.4",
