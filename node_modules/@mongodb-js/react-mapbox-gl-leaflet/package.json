    "jsdom-global": "^3.0.2",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.6",
    "leaflet": "^1.4.0",
    "lint-staged": "^8.1.4",
    "mapbox-gl": "^0.53.0",
    "mapbox-gl-leaflet": "0.0.3",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "mongodb-js-precommit": "^2.0.0",
