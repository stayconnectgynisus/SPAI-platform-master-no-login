    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "rollup": "^1.32.1",
